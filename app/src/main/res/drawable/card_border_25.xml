<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="#015005">

    <item>
        <shape android:shape="rectangle">
            <stroke
                android:width="0.5dp"
                android:color="#015005" />
            <corners android:radius="25dp" />
        </shape>
    </item>

    <item android:id="@android:id/mask">
        <shape android:shape="rectangle">
            <stroke
                android:width="0.5dp"
                android:color="#015005" />
            <corners android:radius="25dp" />
        </shape>
    </item>

</ripple>